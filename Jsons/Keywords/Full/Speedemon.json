{
	"blurb":"Speedemon",
	"triggerData":
	{
		"blurb":"Speedemon",
		"triggerCondition":"Move",
		"triggerRestriction": {
			"$type": "Restrict.Triggering:AllOf:",
			"elements": [
				{
					"$type": "Restrict.Gamestate:CardsMatch:",
					"card": { "$type": "Identify.Cards:CardBefore:" },
					"other": { "$type": "Identify.Cards:ThisCardNow:" }
				},
				{ "$type": "Restrict.Gamestate:MaxPerStack:" },
				{
					"$type": "Restrict.Triggering:Not:",
					"inverted": {
						"$type": "Restrict.Gamestate:StackablesMatch:",
						"firstStackable": { "$type": "Identify.Stackables:StackableCause:" },
						"secondStackable": { "$type": "Identify.Stackables:ThisEffect:" }
					}
				}
			]
		}
	},
	"subeffects":[
		{
			"$type":"Subeffect:TargetThis:"
		},
		{
			"$type":"Subeffect:TargetTriggeringCardsSpace:"
		},
		{
			"$type":"Subeffect:TargetTriggeringCardsSpace:",
			"after":true
		},
		{
			"$type":"Subeffect:AutoSpaceTarget:",
			"blurb":"speeeeeeeeeeeeeeeed",
			"spaceRestriction":
			{
				"$type": "Restrict.Spaces:AllOf:",
				"elements": [
					{
						"$type": "Restrict.Spaces:CanMoveCard:",
						"toMove": { "$type": "Identify.Cards:TargetIndex:" }
					},
					{
						"$type": "Restrict.Spaces:CompareDistance:",
						"distanceTo": { "$type": "Identify.Cards:ThisCardNow:" },
						"number": { "$type": "Identify.Numbers:Constant:", "constant": 1 },
						"comparison": { "$type": "Relationships.Numbers:Equal:" }
					},
					{
						"$type": "Restrict.Spaces:Direction:",
						"from": { "$type": "Identify.Cards:ThisCardNow:" },
						"direction": {
							"$type": "Identify.Spaces:Direction:",
							"from": { "$type": "Identify.Spaces:TargetIndex:", "index": -2 },
							"to": { "$type": "Identify.Spaces:TargetIndex:", "index": -1 }
						}
					}
				]
			}
		},
		{
			"$type":"Subeffect:Move:"
		}
	]	
}