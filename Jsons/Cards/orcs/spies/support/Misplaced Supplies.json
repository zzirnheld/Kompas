									{ "$type": "Restrict.Cards:Character:" },
									{ "$type": "Restrict.Cards:Friendly:" },
									{
										"$type": "Restrict.Spaces:InAOEOf:",
										"card": { "$type": "Identify.Cards:ThisCardNow:" }
									}
								]
							}
						}
					]
				}
			},
			"subeffects": [
				{
					"$type": "Subeffect:AddPips:",
					"xMultiplier": 0,
					"xModifier": 1
				},
				{
					"$type": "Subeffect:DrawX:",
					"xModifier": 1
				},
				{
					"$type": "Subeffect:TargetThis:"
				},
				{
					"$type": "Subeffect:Dispel:"
				}
			]
		},
		{
			"triggerData": {
				"triggerCondition": "Defend",
				"triggerRestriction": {
					"$type": "Restrict.Triggering:AllOf:",
					"elements": [
						{
							"$type": "Restrict.Triggering:CardFitsRestriction:",
