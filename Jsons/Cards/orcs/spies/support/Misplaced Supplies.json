						"$type": "Restrict.Spaces:AllOf:",
						"elements": [
							{ "$type": "Restrict.Spaces:Empty:" },
							{
								"$type": "Restrict.Spaces:AdjacentTo:",
								"space": { "$type": "Identify.Cards:TargetIndex:" }
							}
						]
					}
				},
				{
					"$type": "Subeffect:TargetThis:"
				},
				{
					"$type": "Subeffect:TargetTargetsController:"
				},
				{
					"$type": "Subeffect:Play:"
				}
			]
