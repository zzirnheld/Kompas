{
	"cardType": 67,
	"cardName": "Roth'Ka Spymaster",
	"effText": "Once each turn, you can board->hand a friendly character. If they're a Spy, give them -1 S and draw 1.",
	"effects":[
		{
			"blurb": "Retrieve",
			"activationRestriction": {
				"$type": "Restrict.Players:ActivationRestriction:",
				"elements": [ { "$type": "Restrict.Gamestate:MaxPerTurn:" } ]
			},
			"subeffects": [
				{
					"$type": "Subeffect:CardTarget:",
					"blurb": "Hand a friendly summon",
					"toSearch": { "$type": "Identify.ManyCards:Board:" },
					"cardRestriction": {
						"$type": "Restrict.Cards:AllOf:",
						"elements": [
							{ "$type": "Restrict.Cards:Friendly:" },
							{ "$type": "Restrict.Cards:Character:" },
							{
								"$type": "Restrict.Cards:Not:",
								"negated": {
									"$type": "Restrict.Cards:CardsMatch:",
									"card": { "$type": "Identify.Cards:ThisCardNow:" }
								}
							}
						]
					}
				},
				{
					"$type": "Subeffect:Hand:"
				},
				{
					"$type": "Subeffect:ConditionalEnd:",
					"endIfTrue": {
						"$type": "Restrict.Gamestate:Not:",
						"negated": {
							"$type": "Restrict.Gamestate:CardFitsRestriction:",
							"card": { "$type": "Identify.Cards:TargetIndex:" },
							"cardRestriction": {
								"$type": "Restrict.Cards:Subtypes:",
								"subtypes": [ "Spy" ]
							}
						}
					}
				},
				{
					"$type": "Subeffect:ChangeCardStats:",
					"forbidNotBoard": false,
					"sModifier": -1
				},
				{
					"$type": "Subeffect:DrawX:",
					"xModifier": 1
				}
			]
		}
	],
	"n": 4,
	"e": 8,
	"s": 7,
	"w": 3,
	"subtypes": [
		"Orc",
		"Spy"
	],
	"subtypeText": "Orc Spy"
}