{
	"cardType": 67,
	"cardName": "Hypnosympatheticizer",
	"effText": "Speedemon.\nThe first time each stack I move, move a distinct character 1 space in the same direction.",
	"effects": [
		{
			"blurb": "hyyyypnoooo SYMP",
			"triggerData": {
				"triggerCondition": "Move",
				"triggerRestriction": {
					"$type": "Restrict.Triggering:AllOf:",
					"elements": [
						{
							"$type": "Restrict.Gamestate:CardsMatch:",
							"card": { "$type": "Identify.Cards:CardBefore:" },
							"other": { "$type": "Identify.Cards:ThisCardNow:" }
						},
						{ "$type": "Restrict.Gamestate:MaxPerStack:" }
					]
				}
			},
			"subeffects": [
				{
					"$type": "Subeffect:CardTarget:",
					"blurb": "card to sympa",
					"toSearch": { "$type": "Identify.ManyCards:Board:" },
					"cardRestriction": {
						"$type": "Restrict.Cards:AllOf:",
						"elements": [
							{ "$type": "Restrict.Cards:DistinctName:" },
							{
								"$type": "Restrict.Cards:CanMove:",
								"spaceRestrictionSubeffectIndex": 1
							}
						]
					}
				},
				{
					"$type": "Subeffect:SpaceTarget:",
					"blurb": "where for speed victim to move",
					"spaceRestriction": {
						"$type": "Restrict.Spaces:AllOf:",
						"elements": [
							{
								"$type": "Restrict.Spaces:CanMoveCard:",
								"toMove": { "$type": "Identify.Cards:TargetIndex:" }
							},
							{
								"$type": "Restrict.Spaces:CompareDistance:",
								"distanceTo": { "$type": "Identify.Cards:TargetIndex:" },
								"number": { "$type": "Identify.Numbers:Constant:", "constant": 1 }
							},
							{
								"$type": "Restrict.Spaces:Direction:",
								"from": { "$type": "Identify.Cards:TargetIndex:" },
								"directionToMatch": {
									"$type": "Identify.Spaces:Direction:",
									"from": { "$type": "Identify.Cards:CardBefore:" },
									"to": { "$type": "Identify.Cards:ThisCardNow:" }
								}
							}
						]
					}
				},
				{
					"$type": "Subeffect:Move:"
				}
			]
		}
	],
	"keywords": [
		"Speedemon"
	],
	"n": 1,
	"e": 4,
	"s": 4,
	"w": 4,
	"subtypeText": "Speedemon"
}