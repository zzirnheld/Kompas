{
	"cardType": 67,
	"cardName": "Coach Lap",
	"effText": "Speedemon.\nThe first time each stack a character arrives adjacent to me, their controller moves them 1 space.",
	"effects": [
		{
			"blurb": "Lap! Yeah!",
			"triggerData": {
				"triggerCondition": "Arrive",
				"triggerRestriction": {
					"$type": "Restrict.Triggering:AllOf:",
					"elements": [
						{
							"$type": "Restrict.Spaces:AdjacentTo:",
							"space": { "$type": "Cards:ThisCardNow:" }
						},
						{ "$type": "Core.GamestateRestriction:MaxPerStack:" },
						{
							"$type": "Restrict.Triggering:CardFitsRestriction:",
							"card": { "$type": "Cards:CardBefore:" },
							
						"cardRestriction": {
							"$type": "Restrict.Cards:AllOf:",
							"elements": [
									{ "$type": "Restrict.Cards:Character:" },
									{
										"$type": "Restrict.Cards:Not:",
										"negated": {
											"$type": "Restrict.Cards:CardsMatch:",
											"card": { "$type": "Cards:ThisCardNow:" }
										}
									}
								]
							}
						}
					]
				}
			},
			"subeffects": [
				{
					"$type": "Subeffect:TargetTriggeringCard:"
				},
				{
					"$type": "Subeffect:TargetTargetsController:"
				},
				{
					"$type": "Subeffect:SpaceTarget:",
					"blurb": "next lap?",
					"spaceRestriction": {
						"$type": "Restrict.Spaces:AllOf:",
						"elements": [
							{
								"$type": "Restrict.Spaces:CanMoveCard:",
								"toMove": { "$type": "Cards:TargetIndex:" }
							},
							{
								"$type": "Restrict.Spaces:CompareDistance:",
								"distanceTo": { "$type": "Cards:TargetIndex:" },
								"number": { "$type": "Numbers:Constant:", "constant": 1 }
							}
						]
					}
				},
				{
					"$type": "Subeffect:Move:"
				}
			]
		}
	],
	"keywords": [
		"Speedemon"
	],
	"n": 1,
	"e": 3,
	"s": 4,
	"w": 5,
	"unique": true,
	"subtypeText": "Encouraging Speedemon Person"
}