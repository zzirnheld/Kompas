{
	"cardType": 67,
	"cardName": "Tsuchinoko",
	"effText": "When I'm summoned, if there's X characters in play but NOT in my area, gain X pips.",
	"effects": [
		{
			"triggerData": {
				"triggerCondition": "Play",
				"triggerRestriction": {
					"$type": "Restrict.Triggering:CardsMatch:",
					"card": { "$type": "Identify.Cards:CardBefore:" },
					"other": { "$type": "Identify.Cards:ThisCardNow:" }
				}
			},
			"subeffects": [
				{
					"$type": "Subeffect:SetXByNumberIdentity:",
					"numberIdentity": {
						"$type": "Identify.Numbers:CountCards:",
						"Identify.Cards": { "$type": "Identify.ManyCards:Board:" },
						"cardRestriction": {
							"$type": "Restrict.Cards:AllOf:",
							"elements": [
								{ "$type": "Restrict.Cards:Character:" },
								{
									"$type": "Restrict.Cards:Not:",
									"negated": {
										"$type": "Restrict.Spaces:InAOEOf:",
										"card": { "$type": "Identify.Cards:ThisCardNow:" }
									}
								}
							]
						}
					}
				},
				{
					"$type": "Subeffect:AddPips:"
				}
			]
		}
	],
	"n": 2,
	"e": 11,
	"s": 8,
	"w": 1,
	"spellTypes": [ "Radial" ],
	"radius": 3,
	"subtypeText": "Cryptid Beast"
}