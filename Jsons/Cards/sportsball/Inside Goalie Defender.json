{
	"cardType": 67,
	"cardName": "Inside Goalie Defender",
	"effText": "When you augment your adjacent Avatar, attach their Augment(s) to another character adjacent to me.",
	"effects": [
		{
			"triggerData": {
				"blurb": "Goal Kick",
				"triggerCondition": "Augmented",
				"triggerRestriction": {
					"$type": "Restrict.Triggering:AllOf:",
					"elements": [
						{ "$type": "Core.GamestateRestriction:ThisCardInPlay:" },
						{
							"$type": "Restrict.Triggering:PlayersMatch:",
							"firstPlayer": { "$type": "Players:TriggeringPlayer:" },
							"secondPlayer": { "$type": "Players:FriendlyPlayer:" }
						},
						{
							"$type": "Restrict.Triggering:CardFitsRestriction:",
							"card": { "$type": "Cards:CardBefore:" },
							
						"cardRestriction": {
							"$type": "Restrict.Cards:AllOf:",
							"elements": [
									{ "$type": "Restrict.Cards:Friendly:" },
									{
										"$type": "Restrict.Cards:Location:",
										"locations": [ "Board" ]
									},
									{ "$type": "Restrict.Cards:Avatar:" },
									{
										"$type": "Restrict.Spaces:AdjacentTo:",
										"space": { "$type": "Cards:ThisCardNow:" }
									}
								]
							}
						}
					]
				}
			},
			"subeffects": [
				{
					"$type": "Subeffect:TargetTriggeringCard:"
				},
				{
					"$type": "Subeffect:CardTarget:",
					"blurb": "character to goal-kick Augment to",
					"toSearch": { "$type": "ManyCards:Board:" },
					"cardRestriction": {
						"$type": "Restrict.Cards:AllOf:",
						"elements": [
							{ "$type": "Restrict.Cards:Character:" },
							{
								"$type": "Restrict.Spaces:AdjacentTo:",
								"space": { "$type": "Cards:ThisCardNow:" }
							},
							{
								"$type": "Restrict.Cards:Not:",
								"negated": {
									"$type": "Restrict.Cards:CardsMatch:",
									"card": { "$type": "Cards:TargetIndex:" }
								}
							}
						]
					}
				},
				{
					"$type": "Subeffect:TargetAugments:",
					"targetIndex": -2
				},
				{
					"$type": "Subeffect:ConditionalEnd:",
					"endIfTrue": {
						"$type": "Restrict.Triggering:NumberFitsRestriction:",
						"number": { "$type": "Numbers:TargetCount:" },
						"restriction": {
							"$type": "Core.NumberRestriction:Compare:",
							"comparison": { "$type": "Relationships.Number:LessThanEqual:" },
							"other": { "$type": "Numbers:Constant:", "constant": 2 }
						}
					}
				},
				{
					"$type": "Subeffect:Attach:",
					"targetToAttachTo": 1
				},
				{
					"$type": "Subeffect:LoopWhileHaveTargets:",
					"delete": true,
					"remainingTargets": 2,
					"jumpIndices": [
						4
					]
				}
			]
		}
	],
	"n": 1,
	"e": 5,
	"s": 1,
	"w": 0,
	"subtypeText": "Teammate"
}