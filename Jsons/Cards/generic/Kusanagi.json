{
	"cardType": 65,
	"cardName": "Kusanagi",
	"effText": "The first time each round this character attacks, if their target is behind them, swap their places and give the defender -5 W.",
	"effects": [
		{
			"triggerData": {
				"triggerCondition": "Attack",
				"triggerRestriction": {
					"$type": "Restrict.Triggering:AllOf:",
					"elements": [
						{
							"$type": "Restrict.Triggering:CardsMatch:",
							"card": { "$type": "Identify.Cards:CardBefore:" },
							"other": {
								"$type": "Identify.Cards:AugmentedCard:",
								"ofThisCard": { "$type": "Identify.Cards:ThisCardNow:" }
							}
						},
						{ "$type": "Restrict.Gamestate:MaxPerRound:" }
					]
				}
			},
			"subeffects": [
				{
					"$type":"Subeffect:TargetTriggeringCard:",
					"contextSecondaryCard":true
				},
				{
					"$type": "Subeffect:ConditionalEnd:",
					"endIfTrue": {
						"$type": "Restrict.Gamestate:Not:",
						"negated": {
							"$type": "Restrict.Triggering:CardFitsRestriction:",
							"card": { "$type": "Identify.Cards:TargetIndex:" },
							"cardRestriction": {
								"$type": "Restrict.Spaces:Behind:",
								"card": { "$type": "Identify.Cards:ThisCardNow:" }
							}
						}
					}
				},
				{
					"$type": "Subeffect:ChangeCardStats:",
					"wModifier": -5
				}
			]
		}
	],
	"PlayRestriction": {
		"$type": "Restrict.Play:PlayRestriction:",
		"playAsAugment": true,
		"augmentOnSubtypes": [ "Warrior" ]
	},
	"a": 3,
	"subtypeText": "Sword"
}