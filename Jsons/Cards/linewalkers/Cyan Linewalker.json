{
	"cardType": 67,
	"cardName": "Cyan Linewalker",
	"effText": "LINEWALK.\nWhen I move X spaces, move a distinct card X spaces.",
	"effects": [
		{
			"blurb": "Cyan Linewalked",
			"triggerData": {
				"triggerCondition": "Move",
				"triggerRestriction": {
					"$type": "Restrict.Gamestate:CardsMatch:",
					"card": { "$type": "Identify.Cards:CardBefore:" },
					"other": { "$type": "Identify.Cards:ThisCardNow:" }
				}
			},
			"subeffects": [
				{
					"$type": "Subeffect:CardTarget:",
					"blurb": "for Cyan to move",
					"toSearch": { "$type": "Identify.ManyCards:Board:" },
					"cardRestriction": {
						"$type": "Restrict.Cards:AllOf:",
						"elements": [
							{ "$type": "Restrict.Cards:DistinctName:" },
							{
								"$type": "Restrict.Cards:CanMove:",
								"spaceRestrictionSubeffectIndex": 1
							}
						]
					}
				},
				{
					"$type": "Subeffect:SpaceTarget:",
					"blurb": "where Cyan to move its target",
					"spaceRestriction": {
						"$type": "Restrict.Spaces:AllOf:",
						"elements": [
							{
								"$type": "Restrict.Spaces:CanMoveCard:",
								"toMove": { "$type": "Identify.Cards:TargetIndex:" }
							},
							{
								"$type": "Restrict.Spaces:CompareDistance:",
								"distanceTo": { "$type": "Identify.Cards:TargetIndex:" },
								"number": { "$type": "Identify.Numbers:Constant:", "constant": 1 }
							}
						]
					}
				},
				{
					"$type": "Subeffect:Move:"
				}
			]
		}
	],
	"movementRestriction": {
		"$type": "Restrict.Spaces:MovementRestriction:",
		"movementCosts": [
			{
				"cost": {
					"$type": "Identify.Numbers:Distance:",
					"firstSpace": { "$type": "Identify.Spaces:ContextSpace:" },
					"secondSpace": { "$type": "Identify.Spaces:ContextSpace:", "secondaryContext": true },
					"throughRestriction": { "$type": "Restrict.Spaces:Empty:" }
				}
			},
			{
				"condition": {
					"$type": "Restrict.Gamestate:SpacesFitRestriction:",
					"spaces": { "$type": "Identify.Spaces:ContextSpace:" },
					"spaceRestriction": {
						"$type": "Restrict.Spaces:SameDiagonal:",
						"other": { "$type": "Identify.Spaces:ContextSpace:", "secondaryContext": true }
					}
				},
				"cost": { "$type": "Identify.Numbers:Constant:", "constant": 1 }
			}
		]
	},
	"n": 1,
	"e": 3,
	"s": 6,
	"w": 3,
	"subtypeText": "Linewalker Mage"
}